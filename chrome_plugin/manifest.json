{
  "name": "zenblip",
  "version": "0.2.0",
  "description": "zenblip beta",
  "key":"",
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["deps.min.js", "content.js"]
      //"css": ["font-awesome/css/font-awesome.min.css", "dashboard/css-build/dashboard.css", "content.css"]
      // "css": ["content.css", "content.css"]
    }
  ],
  "minimum_chrome_version": "29",
  "background": { "scripts": [
    "jquery-1.10.2.min.js",
    "channel.js",
    "utils.js",
    "background.js"
    ] 
  },
  "web_accessible_resources": [
    "font-awesome/*",
    "dashboard/*",
    "content.css",
    "jquery-1.10.2.min.js",
    "react-with-addons.min.js",
    "deps.min.js",
    "gmail.js",
    "channel.js",
    "utils.js",
    "main.js",
    "dashboard/js-build/dashboard.js",
    "backgroundMain.js"
  ],
  "permissions":[
    "identity",
    "tabs",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
    // "*://*.google.com/*",
    // "*://*.googleusercontent.com/*",
    // "*://www.googleapis.com/*",
    // "*://zenblip.appspot.com/*",
    // "*://*.zenblip.com/*",
    // "*://*.linkedin.com/*",
  ],
  "content_security_policy": "script-src 'self' https://www.zenblip.com https://zenblip.appspot.com https://talkgadget.google.com; object-src 'self'",
  "oauth2" :{
    "client_id": "709499323932-fiasgo5aeh369gko480h7s17v63glgok.apps.googleusercontent.com",
    "scopes":[
      "email",
      "https://www.googleapis.com/auth/plus.login",
      "https://mail.google.com/"
    ]
  },
  "externally_connectable": {
    "matches": ["*://mail.google.com/*"]
  },
  "manifest_version": 2
}