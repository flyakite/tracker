{
  "name": "zenblip",
  "version": "0.2.8",
  "icons": {
    "128": "128.png"
  },
  "description": "zenblip beta",
  "key":"",
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      
      "js": [
        "deps.min.js", 
        "jquery-1.10.2.min.js",
        "react-with-addons.min.js",
        "utils.js",
        "messenger.js",
        "tracker.js",
        "dashboard/js-build/tracker-dashboard.js",
        "content.js"
        ]
    }
  ],
  "minimum_chrome_version": "29",
  "background": { "scripts": [
    "jquery-1.10.2.min.js",
    "channel.js",
    "utils.js",
    "background.js"
    ] 
  },
  "web_accessible_resources": [
    "font-awesome/*",
    "dashboard/*",
    "content.css",
    "jquery-1.10.2.min.js",
    "react-with-addons.min.js",
    "deps.min.js",
    "gmail.js",
    "messenger.js",
    "tracker.js",
    "channel.js",
    "utils.js",
    "main.js",
    "dashboard/js-build/tracker-dashboard.js",
    "backgroundMain.js"
  ],
  "permissions":[
    "identity",
    "storage",
    "tabs",
    "notifications",
    "webRequest",
    "webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ],
  "content_security_policy": "script-src 'self' https://*.zenblip.com https://zenblip.appspot.com https://talkgadget.google.com https://cdn.mxpnl.com; object-src 'self'",
  "oauth2" :{
    "client_id": "709499323932-fiasgo5aeh369gko480h7s17v63glgok.apps.googleusercontent.com",
    "scopes":[
      "email",
      "https://www.googleapis.com/auth/plus.login",
      "https://mail.google.com/"
    ]
  },
  "externally_connectable": {
    "matches": ["https://mail.google.com/*"]
  },
  "manifest_version": 2
}